package ex04;

interface Printable{
	void print();
}

class Papers{
	private String con;
	public Papers(String con) {this.con = con;}
	public Printable getPrinter() {			
		
		return null;					
	}
}

public class UseLocal_InnerClass {
	public static void main(String[] args) {
		Papers p = new Papers("프린터 : 메시지 출력");
		
		Printable prn = p.getPrinter();
		prn.print();						
	}
}
