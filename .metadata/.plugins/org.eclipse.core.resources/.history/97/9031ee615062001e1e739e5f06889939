package ex01;

class Outer3{
	private int num = 0;
	
	class Member{
		void add(int n) {num+=n;}	//this로 접근 불가.
		int get() {return num;}
	}
}

class Outer4{
	private int num = 0;
	
	class Member{					//동일한 내부 클래스명.
		void add(int n) {num+=n;}	
		int get() {return num;}
	}
}

public class MemberInnerClass {
	public static void main(String[] args) {
		
	}
}
